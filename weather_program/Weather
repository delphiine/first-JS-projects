const WeatherApi = require('./WeatherApi')

class Weather {
    
    constructor(api) {
        this.api = api;
    }

    fetch (city) {
        this.api.fetchWeatherData(city, (weatherData) => {
            this.weatherData = weatherData;
        })
    }

    getWeatherData () {
        return this.weatherData;
    }
}

module.exports = Weather;